		<!-- データ管理機関コード-->
		<jpcoar:nameIdentifier nameIdentifierScheme="ROR">https://ror.org/057zh3y96</jpcoar:nameIdentifier>
		<!-- データ管理機関-->
		<jpcoar:contributorName xml:lang="ja" nameType="Organizational">〇〇大学</jpcoar:contributorName>
		<jpcoar:contributorName xml:lang="en" nameType="Organizational">The University of 〇〇</jpcoar:contributorName>
	</jpcoar:contributor>
