	<!-- データの種別--><!-- ◆COAR語彙のDataset下位項目を指定可能 -->
	<dc:type rdf:resource="http://purl.org/coar/resource_type/63NG-B465">dataset</dc:type>
	<!-- リポジトリ情報--><!-- IRDBハーベスト時に付与される-->
	<!-- リポジトリURI, DOIリンク-->
	<jpcoar:identifier identifierType="DOI">https://doi.org/10.xxxxx/xxxxxxxx</jpcoar:identifier>
	<!-- データNo. -->
	<jpcoar:relation relationType="isIdenticalTo">
		<jpcoar:relatedIdentifier identifierType="Local">12345678</jpcoar:relatedIdentifier>
	</jpcoar:relation>
	
